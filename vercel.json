
{
    
"version" : 2,
"builds": [
    {
        "src":"OnlineWork/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxlambdaSize":"15mb", 
                    "runtime":"pythom3.9"}
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static_build",
        "config":{
            "distDir":"staticfiles_build"
        }
    }
],
"routes":[
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
        "src":"/(.*)",
        "dest": "OnlineWork/wsgi.py"
    }
]

}